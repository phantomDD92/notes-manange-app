{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "build:lib": "npm run build:check && npm run build:package",
    "build:package": "NODE_ENV=production rollup -c && node build-meta && node api-generator/build-api && gulp build-styles",
    "build:check": "npm run format:check && npm run security:check",
    "format": "prettier --write \"**/*.{js,vue,d.ts}\" --cache",
    "format:check": "prettier --check \"**/*.{js,vue,d.ts}\""
  },
  "dependencies": {
    "@babel/preset-env": "^7.24.3",
    "@docsearch/js": "^3.6.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-istanbul": "^1.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.24.0",
    "nuxt": "^3.11.1",
    "pinia": "^2.1.7",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^3.50.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.72.0",
    "vitest": "^1.4.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@pinia/nuxt": "^0.5.1",
    "@rollup/plugin-babel": "^6.0.4",
    "autoprefixer": "^10.4.19",
    "nuxt-gtag": "^2.0.5",
    "nuxt-primevue": "^0.3.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3"
  }
}
